{
  "@deathbeds/jupyterlab-fonts:fonts": {
    "styles": {
      ":root": {
        "--jp-code-font-family": "'Fira Code Regular'",
        "--jp-content-font-family": "'Fira Code Light'",
        "--jp-ui-font-family": "'Fira Code Light'"
      }
    }
  },
  "@jupyterlab/completer-extension:manager": {
    "availableProviders": {
      "lsp": 1000,
      "CompletionProvider:context": 500,
      "CompletionProvider:kernel": 550
    },
    "providerTimeout": 1000,
    "showDocumentationPanel": true,
    "autoCompletion": true,
    "suppressIfInlineCompleterActive": true
  }
}
